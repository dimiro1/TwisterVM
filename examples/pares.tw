# pares e impares
.main
  store_s " é par!" $0
  store_s " é impar!" $1
  store_n 0 $0 # n
  store_n 10 $1 # max
  store_n 2 $2
LOOP:
  gt_n $0 $1 END
  mod_n $0 $2 $4
  zero_n $4 PAR
  goto IMPAR
PAR:
  print_n $0
  put_s $0
  inc_n $0
  goto LOOP
IMPAR:
  print_n $0
  put_s $1 
  inc_n $0
  goto LOOP
END:
  halt
.end
